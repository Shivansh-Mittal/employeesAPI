{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "redirects": [
        {
            "source": "/employee",
            "destination": "/routes/employee"
        }
    ],
    "routes": [
        {
            "source": "/(.*)",
            "destination": "/routes/"
        }
    ]
}